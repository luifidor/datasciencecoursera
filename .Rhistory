f_states <- levels(factor(data$State))
r <- NULL
for (s in f_states) {
t <- data[data$State == s,]
rank_v = order(t[,outcome_id])
t <- cbind(t[rank_v,], rank = 1:length(rank_v), length(rank_v):1)
if (is.null(r)) {
r <- t
} else {
r <- rbind(r, t)
}
}
#r[,c(7,2,outcome_id,47,48)]
